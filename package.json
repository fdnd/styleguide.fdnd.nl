{
  "name": "styleguide",
  "version": "0.0.1",
  "description": "It's a styleguide for FDND!",
  "source": "src/index.html",
  "targets": {
    "default": {
      "publicUrl": "https://styleguide.fdnd.nl/",
      "distDir": "./docs"
    }
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true
    }
  },
  "scripts": {
